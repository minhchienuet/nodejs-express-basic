extends ../layouts/master

block content
  .panel.panel-default
    .panel-heading
      h1= title
    .panel-body 
      table.table.table-bordered.table-hover.table-striped
        thead
          tr
            th Name
            th Username
            th Email
        tbody
          each user, i in users
            tr
              td #{user.name}
              td #{user.username}
              td #{user.email}